node {
  try {
    stage('checkout') {
      echo "doing some cleanup..."
    }
    stage('prepare') {
      echo "doing some cleanup..."
    }
    stage('compile') {
      echo "doing some cleanup..."
    }
    stage('test') {
      echo "doing some cleanup..."
    }
    stage('package') {
      echo "doing some cleanup..."
    }
    stage('publish') {
      echo "doing some cleanup..."
    }
  } finally {
    stage('cleanup') {
      echo "doing some cleanup..."
    }
  }
}